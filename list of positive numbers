n=int(input('enter the number of items in the list'))
list1=[]
for i in range(1,n+1):
    ele=int(input('enter the element'))
    list1.append(ele)
print("input list1=",list1)
list2=[]
for value in list1:
    if value>=0:
        list2.append(value)
print("The list of positive numbers list2=",list2)
